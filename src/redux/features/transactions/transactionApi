import { baseUrlApi } from './../../api/baseUrlApi';

const transactionApi = baseUrlApi.injectEndpoints({
  endpoints: (build) => ({
    getAllTransaction: build.query({
      query: () => "/transactions",
    }),
     getAllCandidates: build.query({
      query: () => "/apply/candidates",
 
    }),
  }),
});

export const { useGetAllTransactionQuery,useGetAllCandidatesQuery } = transactionApi;
